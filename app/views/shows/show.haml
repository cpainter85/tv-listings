-if @show.show_listings.empty?
  ="No Show Listings for #{@show.name}"
-else
  -@show.show_listings.each do |show_listing|
    ="#{show_listing.show.name} is showing on #{show_listing.channel.name} on #{show_listing.date.strftime('%m/%d/%Y'g)}"
=link_to 'Edit', edit_show_path
=link_to 'New Show Listing', new_show_listing_path
